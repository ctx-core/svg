{
	"name": "@ctx-core/svg",
	"version": "18.4.5",
	"description": "ctx-core svg",
	"keywords": [
		"ctx-core",
		"svg"
	],
	"homepage": "https://github.com/ctx-core/svg#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/svg.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./node",
		"./svg_content_height",
		"./svg_content_width",
		"./svg_height",
		"./svg_margin",
		"./svg_matrix2d__set",
		"./svg_matrix2d_o",
		"./svg_width"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"browser": "./index.browser.js",
			"default": "./index.js",
			"node": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.0.5",
		"@ctx-core/nanostores": "^7.1.6",
		"@ctx-core/object": "^30.2.0",
		"htmlparser2": "^9.0.0"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}