{
	"name": "@ctx-core/svg",
	"version": "12.0.24",
	"description": "ctx-core svg",
	"keywords": [
		"ctx-core",
		"svg"
	],
	"homepage": "https://github.com/ctx-core/svg#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.1.1",
		"@ctx-core/error": "workspace:^10.1.5",
		"@ctx-core/function": "workspace:^19.1.11",
		"@ctx-core/object": "workspace:^20.2.3",
		"@ctx-core/store": "workspace:^26.0.19",
		"domhandler": "^4.2.0",
		"domutils": "^2.7.0",
		"htmlparser2": "^6.1.0",
		"resolve": "^1.20.0",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.13",
		"@ctx-core/dev-tools": "workspace:^6.0.15",
		"@types/express": "4.17.12",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
