{
	"name": "@ctx-core/svg",
	"version": "16.0.5",
	"description": "ctx-core svg",
	"keywords": [
		"ctx-core",
		"svg"
	],
	"homepage": "https://github.com/ctx-core/svg#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/svg.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"browser": {
				"import": "./dist/index.browser.js"
			},
			"node": {
				"import": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.38",
		"@ctx-core/error": "workspace:^11.1.14",
		"@ctx-core/function": "workspace:^20.2.17",
		"@ctx-core/object": "workspace:^22.0.5",
		"@ctx-core/store": "workspace:^27.0.52",
		"dom-serializer": "^1.3.2",
		"domhandler": "^4.3.0",
		"domutils": "^2.8.0",
		"htmlparser2": "^7.2.0",
		"resolve": "^1.20.0"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.92",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/express": "4.17.13",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "index.js"
}
