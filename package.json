{
	"name": "@ctx-core/svg",
	"version": "13.0.8",
	"description": "ctx-core svg",
	"keywords": [
		"ctx-core",
		"svg"
	],
	"homepage": "https://github.com/ctx-core/svg#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module)\nmodule.exports = require('./index.js')\" > index.cjs",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.7",
		"@ctx-core/error": "workspace:^11.0.7",
		"@ctx-core/function": "workspace:^20.0.6",
		"@ctx-core/object": "workspace:^21.0.6",
		"@ctx-core/store": "workspace:^27.0.5",
		"domhandler": "^4.2.0",
		"domutils": "^2.7.0",
		"esm": "^3.2.25",
		"htmlparser2": "^6.1.0",
		"resolve": "^1.20.0",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.30",
		"@ctx-core/dev-tools": "workspace:^7.0.8",
		"@types/express": "4.17.13",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
