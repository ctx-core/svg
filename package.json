{
	"name": "@ctx-core/svg",
	"version": "14.0.0",
	"description": "ctx-core svg",
	"keywords": [
		"ctx-core",
		"svg"
	],
	"homepage": "https://github.com/ctx-core/svg#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/svg/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.24",
		"@ctx-core/error": "workspace:^11.0.19",
		"@ctx-core/function": "workspace:^20.2.4",
		"@ctx-core/object": "workspace:^21.0.23",
		"@ctx-core/store": "workspace:^27.0.37",
		"dom-serializer": "^1.3.2",
		"domhandler": "^4.2.2",
		"domutils": "^2.8.0",
		"htmlparser2": "^7.2.0",
		"resolve": "^1.20.0",
		"svelte": "3.44.1"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.77",
		"@ctx-core/dev-tools": "workspace:^7.2.1",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.108",
		"@types/express": "4.17.13",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}