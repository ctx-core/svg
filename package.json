		"@ctx-core/object": "workspace:^19.0.3",
		"@ctx-core/store": "workspace:^25.1.4",
		"domhandler": "^4.2.0",
		"domutils": "^2.6.0",
		"htmlparser2": "^6.1.0",
		"resolve": "^1.20.0",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^7.0.7",
		"@ctx-core/dev-tools": "workspace:^5.0.4",
		"@types/express": "4.17.12",
